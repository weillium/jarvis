{
    "compilerOptions": {
      "target": "ES2022",
      "lib": ["es2022"],
      "module": "ESNext",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "strict": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["**/*.ts"]
  }